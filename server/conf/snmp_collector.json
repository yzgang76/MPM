{
  "mpm_console": {
    "server": "http://localhost:3000",
    "url": "/V1.0/domains/mpm-n/kpis/ID"
  },
  "collection": [
    {
      "device_info": {
        "type": "Windows_Host",
        "name": "localhost",
        "IP": "127.0.0.1",
        "community": "public",
        "version": "2c"
      },
      "OIDs": [
        {
          "method": "get",
          "name": "Physical memory utilization",
          "formula": "[.1.3.6.1.2.1.25.2.3.1.6.4]/[.1.3.6.1.2.1.25.2.3.1.5.4]",
          "unit":"%",
          "interval": 300
        },
        {
          "method": "walk",
          "aggregation": "avg",
          "name": "Avg CPU Utilization",
          "formula": "[.1.3.6.1.2.1.25.3.3.1.2]",
          "description":"Average CPU Utilization in 5 minutes",
          "unit":"%",
          "interval": 300
        },
        {
          "method": "get",
          "aggregation": "delta",
          "name": "Bytes In",
          "formula": "[.1.3.6.1.2.1.2.2.1.10.12]",
          "unit":"Byte",
          "interval": 300
        },
        {
          "method": "get",
          "aggregation": "delta",
          "name": "Bytes Out",
          "formula": "[.1.3.6.1.2.1.2.2.1.16.12]",
          "unit":"Byte",
          "interval": 300
        }
      ]
    }
  ]
}

